/Users/terungwakombol/terkplumber/src/page-components/Home/index.tsx",[],[],"/Users/terungwakombol/terkplumber/src/page-components/index.ts",[],[],"/Users/terungwakombol/terkplumber/pages/api/first_section.ts",[],[],"/Users/terungwakombol/terkplumber/pages/api/second_section.ts",[],[],"/Users/terungwakombol/terkplumber/pages/api/third_section.ts",[],[],"/Users/terungwakombol/terkplumber/pages/blog.tsx",[],[],"/Users/terungwakombol/terkplumber/pages/contact.tsx",[],[],"/Users/terungwakombol/terkplumber/pages/works.tsx",[],[],"/Users/terungwakombol/terkplumber/src/__mock__/mockIntersectionObserver.ts",[],[],"/Users/terungwakombol/terkplumber/src/components/Button/icon/icon.constants.ts",[],[],"/Users/terungwakombol/terkplumber/src/components/Button/icon/index.tsx",[],[],"/Users/terungwakombol/terkplumber/src/components/Flip-card/Flip-card.test.tsx",[],[],"/Users/terungwakombol/terkplumber/src/components/Flip-card/index.tsx",[],[],"/Users/terungwakombol/terkplumber/src/components/Horizontal-scroller/Horizontal-scroller.test.tsx",[],[],"/Users/terungwakombol/terkplumber/src/components/Horizontal-scroller/index.tsx",["240"],[],"import React, { FC, ReactElement, useRef, createRef, useEffect } from \"react\";\nimport { FlipCard } from \"@components/Flip-card\";\nimport cx from \"classnames\";\nimport { useIntersectionTracker } from \"@hooks/useIntersectionTracker/useIntersectionTracker\";\n\nimport { Skills } from \"types\";\n\nimport styles from \"./styles.module.css\";\n\ntype HSProps = {\n  children: Skills[];\n}\nexport const HorizontalScroller:FC<HSProps> = ({children}) => {\n\n  //add a ref to each item element\n  const itemsRef = useRef(Array.from({length: children?.length}, _ => createRef<HTMLLIElement>()));\n\n  //add a ref to each signal element\n  const signalsRef = useRef(Array.from({length: children?.length}, _ => createRef<HTMLLIElement>()));\n\n  const listRef = useRef<HTMLUListElement>(null);\n\nconst activateSignal = (intersectingIndex: Number) => {\n  signalsRef.current.forEach((signal, i) => {\n    signal.current?.classList.toggle(`${styles['active']}`, i === intersectingIndex)\n  });\n}\n\n const theInObHook = useIntersectionTracker(itemsRef, listRef.current!, 0.6, activateSignal)\n\n\nuseEffect(() => {\n  theInObHook;\n},[])\n\n  return(\n    <div className={styles.HS__viewport}>\n      <ul  data-testid=\"list\"  ref={listRef} className={styles.HS__list}>\n        {\n          children?.map((child, idx): ReactElement => (\n            <li className={styles.HS__item} ref={itemsRef.current[idx]} key={idx}>\n              <FlipCard skill={child} />\n            </li>\n          ))\n        }\n      </ul>\n\n      <ul data-testid=\"signal\" className={styles.HS__signalList} aria-hidden>\n        {\n          Array.from({length: children?.length}, (_, idx) => (\n            <li key={idx} ref={signalsRef.current[idx]} className={cx(styles.HS__signal, \"\")}></li>\n          ))\n        }\n      </ul>\n    </div>\n  );\n}\n","/Users/terungwakombol/terkplumber/src/components/Page-Header/page-header.test.tsx",[],[],"/Users/terungwakombol/terkplumber/src/components/SM-Icons/SM-Icons.test.tsx",[],[],"/Users/terungwakombol/terkplumber/src/components/SM-Icons/index.tsx",[],[],"/Users/terungwakombol/terkplumber/src/fileTransformer.js",[],[],"/Users/terungwakombol/terkplumber/src/hooks/useIntersectionTracker/intersectionTrackerHook.test.tsx",["241"],[],"import React, { createRef, useEffect, useRef } from \"react\";\nimport { render } from \"@testing-library/react\";\n\nimport { useIntersectionTracker } from \"./useIntersectionTracker\";\nimport { getObserverOf, intersect } from \"../../intersectioObserver-test-helper/intersectionObserver-test-helper\";\n\n\nconst Observed = () => {\n //add a ref to each item element\n const itemsRef = useRef(Array.from({length: 2}, _ => createRef<HTMLLIElement>()));\n\n //add a ref to each signal element\n const signalsRef = useRef(Array.from({length: 2}, _ => createRef<HTMLLIElement>()));\n\n const listRef = useRef<HTMLUListElement>(null);\n\n const activateSignal = (intersectingIndex: Number) => {\n  signalsRef.current.forEach((signal, i) => {\n    signal.current?.classList.toggle('active', i === intersectingIndex)\n  });\n}\n\n  const theInObHook = useIntersectionTracker(itemsRef, listRef.current!, 0.6, activateSignal)\n  useEffect(() => {\n    theInObHook;\n  },[])\n  return (\n    <div data-testid=\"wrapper\" >\n      <ul ref={listRef} data-testid=\"list\">\n       {\n        Array.from({length: 2}, (_, idx) => (\n          <li key={idx} ref={itemsRef.current[idx]} className=\"\"></li>\n        ))\n       }\n      </ul>\n      <ul data-testid=\"signals\">\n        {\n          Array.from({length: 2}, (_, idx) => (\n            <li key={idx} ref={signalsRef.current[idx]} className=\"\"></li>\n          ))\n        }\n      </ul>\n    </div>\n  );\n};\n\nit(\"creates an observer\", () => {\n  const { getByTestId } = render(<Observed  />);\n  const list = getByTestId(\"list\");\n  console.log('the list',list)\n  const instance = getObserverOf(list);\n\n  expect(instance.observe).toHaveBeenCalledWith(list);\n});\n\nit(\"does not call the callback without intersection\", () => {\n  const callback = jest.fn();\n  const { getByTestId } = render(<Observed />);\n\n  const wrapper = getByTestId(\"wrapper\");\n  intersect(wrapper, false);\n\n  expect(callback).not.toHaveBeenCalled();\n});\n\nit(\"calls the callback on intersection\", () => {\n  const callback = jest.fn();\n  const { getByTestId } = render(<Observed />);\n\n  const wrapper = getByTestId(\"wrapper\");\n  intersect(wrapper, true);\n\n  expect(callback).toHaveBeenCalledTimes(1);\n});\n\nit(\"calls the callback only once\", () => {\n  const callback = jest.fn();\n  const { getByTestId } = render(<Observed  />);\n\n  const wrapper = getByTestId(\"wrapper\");\n  intersect(wrapper, true);\n  intersect(wrapper, false);\n  intersect(wrapper, true);\n\n  expect(callback).toHaveBeenCalledTimes(1);\n});\n\nit(\"unmounts the hook\", () => {\n  const callback = jest.fn();\n\n  const { getByTestId, unmount } = render(<Observed  />);\n  const wrapper = getByTestId(\"wrapper\");\n  unmount();\n  expect(getObserverOf(wrapper).disconnect).toHaveBeenCalledTimes(1);\n});\n","/Users/terungwakombol/terkplumber/src/hooks/useIntersectionTracker/useIntersectionTracker.ts",["242"],[],"import { MutableRefObject, RefObject, useEffect } from \"react\";\n\nexport const useIntersectionTracker = (\n  targetRef: MutableRefObject<RefObject<HTMLLIElement>[]>,\n  root: HTMLUListElement,\n  threshold : number,\n  callback: (idx: number) => void,\n  ): void => {\n\n  useEffect(()=>{\n    //intersection observer\n    const onIntersectionObserved = (entries: IntersectionObserverEntry[]) => {\n      entries.forEach(entry => {\n        if (entry.isIntersecting && entry.intersectionRatio >= threshold) {\n         const intersectingIndex = targetRef.current.findIndex(target => target.current?.outerHTML === entry.target.outerHTML);\n         callback(intersectingIndex);\n        }\n      });\n    };\n\n    const observer = new IntersectionObserver(onIntersectionObserved, {\n      root: root,\n      threshold: threshold,\n    });\n\n    targetRef.current.forEach(item => {\n      observer.observe(item.current!);\n    });\n\n    return () => observer.disconnect();\n\n  },[])\n}\n","/Users/terungwakombol/terkplumber/src/intersectioObserver-test-helper/intersectionObserver-test-helper.ts",[],[],"/Users/terungwakombol/terkplumber/src/page-components/Blog/index.tsx",[],[],"/Users/terungwakombol/terkplumber/src/page-components/Contact/index.tsx",[],[],"/Users/terungwakombol/terkplumber/src/page-components/Home/Home.test.tsx",[],[],"/Users/terungwakombol/terkplumber/src/page-components/Works/index.tsx",[],[],"/Users/terungwakombol/terkplumber/src/routes/client.ts",[],[],{"ruleId":"243","severity":1,"message":"244","line":34,"column":3,"nodeType":"245","endLine":34,"endColumn":5,"suggestions":"246"},{"ruleId":"